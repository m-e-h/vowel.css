
{
	"use": [
    "autoprefixer",
		"postcss-import",
		"postcss-custom-media",
		"postcss-custom-properties",
		"postcss-strip-units",
		"postcss-mixins",
		"postcss-simple-vars",
		"postcss-color-function",
		"postcss-nested",
    "postcss-discard-comments",
    "postcss-discard-empty",
    "stylefmt"
	],
	"input": "index.css",
	"output": "dist/vowel.css",
	"local-plugins": true,
  "autoprefixer": {
    "browsers": "> 5%"
  }
}
